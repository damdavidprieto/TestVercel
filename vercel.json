{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "public/login.html"
    },
    {
      "src": "/home",
      "dest": "/api/home"
    },
    {
	  "src": "/app",
	  "dest": "/api/app"
    }
  ]
}
