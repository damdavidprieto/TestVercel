{
  "version": 2,
  "name": "testvercel-pena",
  "builds": [
    {
      "src": "public/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/login.html"
    },
    {
      "src": "/home",
      "dest": "/api/home"
    }
  ]
}
